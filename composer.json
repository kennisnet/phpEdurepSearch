{
  "name": "kennisnet/edurepsearch",
  "description": "Edurep and Catalogservice search tool",
  "version": "1.0.4",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kennisnet\\Edurep\\": "src/",
      "Tests\\Kennisnet\\Edurep\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-curl": "*",
    "ext-dom": "*",
    "kennisnet/nllom": "^1.1",
    "kennisnet/phpeckcs": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "phpunit/php-code-coverage": "^9.2",
    "phpstan/phpstan": "^0.12.52"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:coverage": "vendor/bin/phpunit --coverage-html .coverage",
    "analyse": "vendor/bin/phpstan analyse"
  }
}
